{
  "name": "ropes",
  "scripts": {
    "postdeploy": "rails db:schema:load db:seed"
  },
  "env": {
    "LANG": {
      "required": false
    },
    "RACK_ENV": {
      "required": false
    },
    "RAILS_ENV": {
      "required": false
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": false
    },
    "SECRET_KEY_BASE": {
      "generator": "secret"
    }
  },
  "addons": [
    "heroku-postgresql:hobby-dev"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/zldavis/heroku-buildpack-nodejs.git#78732a8e04ed90129a0a8350ad9428db8906198c"
    },
    {
      "url": "urn:buildpack:heroku/ruby"
    }
  ]
}
